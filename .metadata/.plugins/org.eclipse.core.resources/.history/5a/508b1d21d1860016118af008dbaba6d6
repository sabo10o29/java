package jpl.ch01.ex09;

public class ex09 {

	public static void main(String args[]){
		System.out.println("フィボナッチ数列");
		Integer fib[] = new Integer[10];
		int lo = 1;
		int hi = 1;
		System.out.println(lo);
		int i = 0;
		while(hi < 50){
			//System.out.println(hi);
			System.out.println(fib[i]);
			hi = lo + hi;	//新しいhi
			lo = hi - lo;	/*新しいloは、（合計-古いlo）
			 				　すなわち、古いhi*/
			i++;
		}
	}
}
